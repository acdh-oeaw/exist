@@ -28,7 +28,7 @@ RUN apt-get update && apt-get install -y --no-install-recommends \
   expat \
   fontconfig \
   liblcms2-2 \
-  ttf-dejavu-core
+  fonts-dejavu-core
 
 FROM gcr.io/distroless/java11-debian11
 
diff --git a/exist-parent/pom.xml b/exist-parent/pom.xml
index a7b8277974..98f912d195 100644
